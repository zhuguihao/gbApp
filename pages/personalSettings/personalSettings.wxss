/**index.wxss**/

.titleView {
  height: 245rpx;
  background-color: #F7F7F7;
}

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfoAvatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfoNickname {
  font-size: 35rpx;
  color: #aaa;
}

.setLine {
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  flex-direction: row;
}

.setLineImg {
  width: 100rpx;
}

.setLine image {
  width: 50rpx;
  height: 50rpx;
  margin: 25rpx;
  border: 1rpx solid #f5f5f9;
}

.setLineMain {
  border-bottom: 1rpx solid #f5f5f9;
  width: 100%;
  display: flex;
  flex-direction: row;
}

.setContent {
  padding-left: 10rpx;
  font-size: 30rpx;
  width: 100%;
  color: #5E5E5E;
}

.rightIcon {
  width: 20rpx;
  margin:0 30rpx 0 3rpx; 
  color: #838B8B;
}

.outLogin{
  margin-top: 30rpx;
}
