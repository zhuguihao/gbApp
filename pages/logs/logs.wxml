<swiper 
  indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}"
  interval="{{interval}}"
  duration="{{duration}}"
  circular="{{circular}}"
  >
  <block wx:for="{{imgUrls}}" wx:key="imgSwipeKey" >
    <swiper-item>
      <image src="{{item}}" class="slide-image" width="375" height="200"/>
    </swiper-item>
  </block>
</swiper>