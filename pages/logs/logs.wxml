<swiper 
  indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}"
  interval="{{interval}}"
  duration="{{duration}}"
  circular="{{circular}}"
  class='swiper'
  >
  <block wx:for="{{imgUrls}}" wx:key="imgSwipeKey" >
    <swiper-item>
      <image src="{{item}}" class="slide-image" class='swipeImg'/>
    </swiper-item>
  </block>
</swiper>

<block wx:for="{{homePageMenu}}" wx:key="homePageMenuKey" class='homePageMenuBlock'>
  <view class='homePageMenu'>
    <view class='homePageMenuTitle'>{{item.name}}</view>
  </view>
</block>