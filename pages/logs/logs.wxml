<swiper 
  indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}"
  interval="{{interval}}"
  duration="{{duration}}"
  circular="{{circular}}"
  class='swiper'
  >
  <block wx:for="{{imgUrls}}" wx:key="imgSwipeKey" >
    <swiper-item>
      <image src="{{item}}" class="slide-image" class='swipeImg'/>
    </swiper-item>
  </block>
</swiper>

<view class='rowBox'>
  <view class='homePageMenu' wx:for="{{homePageMenu}}" wx:key="homePageMenuKey">
    <block>
      <view class='homePageMenuTitle'>{{item.name}}</view>
    </block>
  </view>
</view>

