/* pages/storage/storage.wxss */
.pickerMain { 
  display: -webkit-box;
  height: 100rpx;
  line-height: 100rpx;
  margin-left: 15rpx;
  border-bottom: 1rpx solid #ccc;
}

.picker {
  width: 80%;
  text-align: center;
}

.scanCode {
  width: 256rpx;
  height: 303rpx;
  line-height: 303rpx;
  text-align: center;
  background-color: #f5f5f9;
}

.storageFrom {
  margin: 15rpx 20rpx;
}

.title {
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  /* border-radius: 10rpx; */
  background-color: #ccc;
  padding: 20rpx;
  font-weight: bold;
}

.storageList {
  border: 1rpx solid #ccc;
}

.stoProList {
  /* height: 70rpx; */
  line-height: 70rpx;
  border-bottom: 1rpx solid #ccc;
  margin-left: 15rpx;
}

.stoProList:last-child {
  border: none;
}

.btn{
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}
